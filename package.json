{
  "name": "@biopinda/biodiversidade-online",
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/papaparse": "^5.5.2",
    "@types/semver": "^7.7.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mongodb-memory-server": "^11.0.1",
    "prettier": "^3.8.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "@anthropic-ai/sdk": "^0.71.2",
    "swagger-jsdoc": "^6.2.8",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "hono": "^4.11.4"
  },
  "packageManager": "bun@1.2.21",
  "private": true,
  "scripts": {
    "ingest:occurrences": "bun run --filter @darwincore/ingest occurrences",
    "ingest:flora": "bun run --filter @darwincore/ingest flora",
    "ingest:fauna": "bun run --filter @darwincore/ingest fauna",
    "ingest:test": "bun run --filter @darwincore/ingest test",
    "transform:taxa": "bun run --filter @darwincore/transform taxa",
    "transform:occurrences": "bun run --filter @darwincore/transform occurrences",
    "transform:execute": "bun run --filter @darwincore/transform execute",
    "transform:check-lock": "bun run --filter @darwincore/transform check-lock",
    "load:fauna-ameacada": "bun run --filter @darwincore/transform load:fauna-ameacada",
    "load:plantae-ameacada": "bun run --filter @darwincore/transform load:plantae-ameacada",
    "load:fungi-ameacada": "bun run --filter @darwincore/transform load:fungi-ameacada",
    "load:invasoras": "bun run --filter @darwincore/transform load:invasoras",
    "load:catalogo-ucs": "bun run --filter @darwincore/transform load:catalogo-ucs",
    "enrich:ameacadas": "bun run --filter @darwincore/transform enrich:ameacadas",
    "enrich:invasoras": "bun run --filter @darwincore/transform enrich:invasoras",
    "enrich:ucs": "bun run --filter @darwincore/transform enrich:ucs",
    "web:dev": "bun run --filter @biodiversidade/web dev",
    "web:build": "bun run --filter @biodiversidade/web build",
    "web:preview": "bun run --filter @biodiversidade/web preview",
    "web:start": "bun run --filter @biodiversidade/web start",
    "format": "prettier --write . --experimental-cli",
    "typecheck": "bun tsc --build && cd packages/web && bunx astro check --minimumSeverity error",
    "typecheck:fast": "bunx tsc --build --pretty false",
    "prepare": "husky"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "catalog": {
      "@types/node": "^22.8.4",
      "bson": "^6.7.0",
      "cli-progress": "^3.12.0",
      "@types/cli-progress": "^3.11.6",
      "extract-zip": "^2.0.1",
      "fast-xml-parser": "^4.4.1",
      "mongodb": "^7.0.0",
      "papaparse": "^5.4.1",
      "@types/papaparse": "^5.5.2",
      "@types/bun": "^1.1.8",
      "typescript": "^5.7.3"
    }
  },
  "lint-staged": {
    "*": "bunx prettier -w --experimental-cli"
  },
  "overrides": {
    "prismjs": "^1.30.0",
    "jsondiffpatch": "^0.7.2",
    "h3": "^1.15.5",
    "diff": "^5.2.2",
    "devalue": "^5.6.2",
    "hono": "^4.11.4"
  },
  "patchedDependencies": {
    "mongodb-mcp-server@1.0.1": "patches/mongodb-mcp-server@1.0.1.patch"
  }
}
